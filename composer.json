{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
	{
	    "type": "package",
	    "package": {
                "name": "desandro/imagesloaded",
		"version": "3.2.0",
		"type": "drupal-library",
		"dist": {
		    "url": "https://github.com/desandro/imagesloaded/archive/v3.2.0.zip",
		    "type": "zip"
	 	}
            }
        },
        {
	    "type": "package",
	    "package": {
                "name": "desandro/masonry",
		"version": "3.3.2",
		"type": "drupal-library",
		"dist": {
		    "url": "https://github.com/desandro/masonry/archive/v3.3.2.zip",
		    "type": "zip"
	 	}
            }
        }

    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "desandro/imagesloaded": "^4.1",
        "desandro/masonry": "^4.2",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^1.26",
        "drupal/bootstrap": "^3.17",
        "drupal/business_rules": "^1.0",
        "drupal/components": "^1.0",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.6.0",
        "drupal/dropzonejs": "^2.0",
        "drupal/entity_browser": "2.x-dev",
        "drupal/entity_embed": "^1.0",
        "drupal/field_group": "^3.0",
        "drupal/file_browser": "^1.10",
        "drupal/ief_table_view_mode": "^2.0",
        "drupal/inline_entity_form": "^1.0",
        "drupal/paragraphs": "1.x-dev",
        "drupal/pathauto": "^1.3",
        "drupal/rabbit_hole": "^1.0",
        "drupal/simplify": "^1.0",
        "drush/drush": "^9.6",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
	"patches": {
            "drupal/paragraphs": {
                "Fixes infinite loop on update": "https://www.drupal.org/files/issues/2019-03-21/paragraphs-update-endless-loop-3041868-23.patch"
            },
            "drupal/core": {
	        "Fixes language issue": "https://www.drupal.org/files/issues/2019-02-28/language-2718657-17.patch"
            }
	}
    }
}
